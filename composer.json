    "wpengine/advanced-custom-fields-pro": "*",
    "djboris88/timber-debugger": "^1.1",
    "hellonico/timber-dump-extension": "^1.0"
  },
  "license": "GPL-2.0+",
  "type": "wordpress-theme",
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
